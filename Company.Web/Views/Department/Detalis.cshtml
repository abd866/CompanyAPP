@model DepartmentDTO

<form asp-action="Create">
    <div class="form-group">
        <label class="col-form-label" asp-for="code"></label>
        <input class="form-control" asp-for="code" type="text" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" type="text" readonly />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="CreateAT"></label>
        <input class="form-control" asp-for="CreateAT" type="text" readonly />
    </div>
    <div class="form-group m-2">
        <a class="btn btn-light" asp-controller="Department" asp-action="Index">Back To List</a>

    </div>
</form>