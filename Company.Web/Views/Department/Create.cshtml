@model Department
<h1 class="p-2">Create New Department</h1>

<form  asp-action="Create">
    <div class="form-group">
        <label class="col-form-label" asp-for="code"></label>
        <input class="form-control" asp-for="code" type="text" />
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control" asp-for="Name" type="text" />
